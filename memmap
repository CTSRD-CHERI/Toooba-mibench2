/* memmap */
MEMORY
{
    rom : ORIGIN = 0x80000000, LENGTH = 0x00040000
    ram : ORIGIN = 0x80040000, LENGTH = 0x00040000
    h : ORIGIN = 0x80080000, LENGTH = 0x00040000
}

SECTIONS
{
    .text : { *(.text*) } > rom
    .bss : { *(.bss*) } > ram
    .heap : { *(.heap*) } > h
}
    end = ORIGIN(h) + LENGTH(h);

heapCurrent = ORIGIN(h);

_stacktop = ORIGIN(ram) + LENGTH(ram);
